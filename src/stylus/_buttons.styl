.btn
    position relative
    display inline-block
    padding 6px 12px
    margin-bottom 0
    font-size 14px
    font-weight normal
    line-height 1.42857143
    text-align center
    white-space nowrap
    vertical-align middle
    cursor pointer
    -webkit-user-select none
    -moz-user-select none
    -ms-user-select none
    user-select none
    background-image none
    border 1px solid transparent
    border-radius 4px

    i
        font-size 18px
        position absolute
        top 25%

.btn.disabled,
.btn[disabled]
    pointer-events none
    cursor not-allowed
    box-shadow none
    opacity .65

