.alert
    padding 15px
    margin-bottom 20px
    border 1px solid transparent
    border-radius 4px
    h4
        margin-top 0
        color inherit
    .alert-link
        font-weight bold
    & > p
        & + p
            margin-top 5px

.alert > p,
.alert > ul
    margin-bottom 0

.alert-dismissable,
.alert-dismissible
    padding-right 35px

.alert-dismissable .close,
.alert-dismissible .close
    position relative
    top -2px
    right -21px
    color inherit

.alert-success
    color #3c763d
    background-color #dff0d8
    border-color #d6e9c6
